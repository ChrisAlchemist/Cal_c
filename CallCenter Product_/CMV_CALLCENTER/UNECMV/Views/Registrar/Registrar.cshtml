                            @Html.TextArea("1", new { @class = "form-control", @placeholder = "Descripción", @id = "spanDescr", @maxlength = "1000", @rows = "5", @onkeypress = "descripcionReporte()", @readOnly = "" })
                        </div>
                    </div>
                </div>
                <ul class="pager wizard">
